                        android:textColor="#000000"
                        android:textSize="15sp" />

                    <TextView
                        android:id="@+id/percent6"
                        android:layout_width="60dp"
                        android:layout_height="match_parent"
                        android:layout_marginRight="2dp"
                        android:layout_weight="0"
